{
    "editor.tabSize": 2,
     "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.git": true,
        "**/dist": true
    }
}